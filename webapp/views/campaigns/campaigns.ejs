<%= campaigns.title %>
<div class="container">
<h2 class="center-align teal-text"> TODOS LOS EVENTOS </h2>

<div class="center-btn">
  <a href="/campaign/create" class="btn btn-success"><span class="fa fa-sign-in"></span>INICIAR NUEVA CAMPAÑA</a>
  <br><br>
</div>

<div class="row">
  <% let i = 0
  campaigns.forEach((campaign) => { %>
  <div class="col s12 m6">
    <div class="card hoverable">
      <div class="card-image">
        <a href="/campaign/<%= campaign._id %>/detail" class="btn-floating halfway-fab waves-effect waves-light red"><i class="material-icons">add</i></a>
      </div>
      <div class="card-content">
        <div class="multipleMap" id="map<%= i%>"></div>
        <br>
        <!-- <div class="center-left"> -->
          <h6 class="teal-text center-align"><u><b>CAMPAÑA</b></u></h6>
          <p><b><%= campaign.title %></b></p>
          <h6 class="teal-text"><b>Descripción:</b></h6>
            <p><%= campaign.description %></p>
          <h6 class="teal-text"><b>Ciudad:</b></h6>
          <p><%= campaign.city %></p>
          <h6 class="teal-text"><b>¿Por cuanto?</b> <%= campaign.price %>€</h6>
      <!-- </div> -->
      </div>
    </div>
  </div>
  <% i++ }) %>
</div>
</div>

<script type="text/javascript">
var myCampaing = <%- JSON.stringify(campaigns) %>;
</script>
<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCE3wzxrBK16zMljFyGq294DTotwbl85TY&libraries=places"></script>
<script type="text/javascript" src="/javascript/mapDrawAll.js"></script>
